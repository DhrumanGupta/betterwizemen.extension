{
  "name": "Better Wizemen",
  "description": "Makes wizemen a tad better :)",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.wizemen.net/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "injections/noCaptcha.js"
  ],
  "permissions": [
    "*://*.wizemen.net/*"
  ]
}
